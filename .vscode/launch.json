{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Migration - Rs",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/migration/target/debug/migration.exe",
      "args": [],
      "cwd": "${workspaceRoot}/migration",
      "envFile": "${workspaceFolder}/migration/.env",
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "preLaunchTask": "Build",
      "stopAtEntry": false
    },
    {
      "name": "Debug Migration - Py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/python_migration/upload.py",
      "console": "integratedTerminal"
    }
  ]
}
